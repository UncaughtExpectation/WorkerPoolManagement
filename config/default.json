{
    "server": {
        "port": 3000,
        "requestBodyLimit": "500mb"
    },
    "logging": {
        "level": "info"
    },
    "workerPool": [
        {
            "poolName": "CPU",
            "workerScript": "./workers/exampleWorker_CPULoad.js",
            "workerCount": 1,
            "workerMemoryLimit": 4048
        },
        {
            "poolName": "MEM",
            "workerScript": "./workers/exampleWorker_MemoryUsage.js",
            "workerCount": 1,
            "workerMemoryLimit": 4048
        }
    ],
    "logWorkerStats": true
}